#include <stdio.h>
#include <string.h>

int main()
{
	char Str[1000];
	scanf("%s", Str);

	int len = strlen(Str);

	bool Ans = false;
	char LastChar = Str[0];
	int LxNum = 1;

	for (int i = 1; i < len; i++)
	{
		if (Str[i] == LastChar)
		{
			LxNum++;
		}
		else
		{
			LxNum = 1;
			LastChar = Str[i];
		}

		if (LxNum >= 7)
			Ans = true;
	}

	if (Ans)
		printf("Yes");
	else
		printf("No");

	return 0;
}