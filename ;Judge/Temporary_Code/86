#include <stdio.h>
#include <string.h>

int main()
{
	char Str1[200], Str2[200];
	scanf("%s", Str1);
	scanf("%s", Str2);

	if (strstr(Str1, Str2))
	{
		printf("%s is substring of %s\n", Str2, Str1);
	}
	else if (strstr(Str2, Str1))
	{
		printf("%s is substring of %s\n", Str1, Str2);
	}
	else
	{
		printf("No substring\n");
	}

	return 0;
}