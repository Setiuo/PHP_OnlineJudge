#include <stdio.h>
#include <math.h>

int x, ans;
double n, m, sum;

int main()
{
	scanf("%lf %lf", &n, &m);

	double T = m * (0.8 + 0.6 + 0.55);
	sum = (ceil((50 - T) / 0.5 / m)) * m * 0.5 + T;

	if (n >= sum)
	{
		ans = 3 + ceil((50 - T) / 0.5 / m);

		while (sum + m * 0.5 < n)
		{
			sum += m * 0.5;
			ans++;
		}
	}
	else
	{
		n -= 10;

		if (n >= m * 0.8)
			ans++;
		if (n >= m * (0.8 + 0.6))
			ans++;
		if (n >= m * (0.8 + 0.6 + 0.55))
			ans++;

		n -= m * (0.8 + 0.6 + 0.55);

		while (n)
		{
			if (n - m * 0.5 >= 0)
			{
				ans++;
				n -= m * 0.5;
			}
			else
				break;
		}
	}

	printf("%d", ans);

	return 0;
}