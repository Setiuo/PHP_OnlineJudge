#include <iostream>

using namespace std;
int Error = 0;

int pri(char op)
{
	int num = 1;

	if (op == '+' || op == '-')
		num = 1;
	else if (op == '*' || op == '/')
		num = 2;

	return num;
}
int compute(int x, char c, int y)
{
	int num = 0;

	switch (c)
	{
	case '+':
	{
		num = x + y;
		break;
	}
	case '-':
	{
		num = x - y;
		break;
	}
	case '*':
	{
		num = x * y;
		break;
	}
	case '/':
	{
		if (y == 0)
		{
			Error = 1;
			break;
		}
		num = x / y;
		break;
	}
	default:
	{
		Error = 2;
		break;
	}
	}

	return num;
}
int main()
{
	int a = 0, b, c;
	char op1 = '+', op2;

	cin >> b;
	while (true)
	{
		cin >> op2;

		if (op2 == '=')
			break;
		else if (op2 != '+' && op2 != '-' && op2 != '*' && op2 != '/')
		{
			Error = 2;
			break;
		}

		cin >> c;


		if (pri(op1) >= pri(op2))
		{
			a = compute(a, op1, b);
			op1 = op2;
			b = c;
		}
		else
		{
			b = compute(b, op2, c);
		}
	}

	if (Error == 0)
		cout << compute(a, op1, b);
	else if (Error == 2)
		cout << "Error";
	else if (Error == 1)
		cout << "No 0";

	return 0;
}