import sys

for line in sys.stdin:
    a = line.split();

n = int(a[0])
i = 1;
while i <= n:
    j = 1;
    while j <= (n - i):
        print(" ", end='');
        j += 1;
    j = 1;

    while j <= i:
        print(j, end='');
        j += 1;

    j = i - 1;
    while j >= 1:
        print(j, end='');
        j -= 1;

    print ();
    i += 1;
