#include <stdio.h>

int main()
{
	int Num[2];
	char Ch;

	scanf("%d %d %c", &Num[0], &Num[1], &Ch);
	switch (Ch)
	{
	case '+':
	{
		printf("%d", Num[0] + Num[1]);
		break;
	}
	case '-':
	{
		printf("%d", Num[0] - Num[1]);
		break;
	}
	case '*':
	{
		printf("%d", Num[0] * Num[1]);
		break;
	}
	case '/':
	{
		if (Num[1] == 0)
		{
			printf("Divided by zero!");
		}
		else
		{
			printf("%d", Num[0] / Num[1]);
		}
		break;
	}
	default:
	{
		printf("Invalid operator!");
	}
	}
	return 0;
}
