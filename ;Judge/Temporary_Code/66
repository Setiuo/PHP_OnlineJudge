import java.util.Scanner;
public class  Main
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
			for ( int i = 1 ; i <= n ; i++ )
		{
			String aaa=sc.next();
			int a=0,len=aaa.length();
			for(int j=1;j<=len;j++)
			{
				a += aaa.charAt(j-1) - '0' ;
			}
			if(a%3==0)
				System.out.println("YES");
			else
				System.out.println("NO");
		}
		
		
		
	}
}
