#include <stdio.h>
#include <string.h>

int main()
{
	char Str[100];

	gets(Str);

	int len = strlen(Str);
	char Ans[100];

	int i = 0;
	for (; i < len; i++)
	{
		if (Str[i] >= 'a' && Str[i] <= 'z')
		{
			Ans[i] = (Str[i] - 'a' + 1) % 26 + 'a';
		}
		else if (Str[i] >= 'A' && Str[i] <= 'Z')
		{
			Ans[i] = (Str[i] - 'A' + 1) % 26 + 'A';
		}
		else
		{
			Ans[i] = Str[i];
		}
	}
	Ans[i] = '\0';

	printf("%s", Ans);
	return 0;
}