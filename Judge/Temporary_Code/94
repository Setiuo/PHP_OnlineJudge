#include <stdio.h>
#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
	unsigned long Num[3];
	scanf("%lu%lu%lu", &Num[0], &Num[1], &Num[2]);

	sort(Num, Num + 3);

	if (Num[0] + Num[1] > Num[2] && Num[1] - Num[0] < Num[2])
		printf("Yes");
	else
		printf("No");

	return 0;
}