#include <stdio.h>
#include <iostream>

using namespace std;

int main()
{
    int n = 0;
    cin>>n;

    int ans1 = 0;
    int ans2 = 555555555;

    for(int i = 0; i < n; i++)
    {
        int j;
        cin>>j;
        ans1 = max(j, ans1);
        ans2 = min(j, ans2);
    }

    cout<<ans1<<" "<<ans2;
    return 0;
}
