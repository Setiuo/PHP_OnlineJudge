#include <cstdio>
#include <math.h>
#include <algorithm>

using namespace std;

int allNum[10000000];

int main()
{
	int n;
	scanf("%d", &n);

	int block = (int)sqrt(n);

	for (int i = 1; i <= n; i++)
	{
		scanf("%d", &allNum[i]);
	}

	int l, r;
	while (~scanf("%d%d", &l, &r))
	{
		int ans = 0;

		for (int i = l; i <= r; i++)
		{
			ans = max(allNum[i], ans);
		}


		printf("%d\n", ans);
	}

	return 0;
}