#include <iostream>
#include <algorithm>

using namespace std;

typedef long long ll;

ll m_pow(ll base, ll p)
{
	ll ans = 1;
	for (int i = 0; i < p; i++)
		ans *= base;

	return ans;
}

int m_lg(ll t)
{
	int ans = 0;
	while (t)
	{
		t /= 10;
		ans++;
	}

	return ans;
}
int main()
{
	ll w, m, k, ans = 0, p, t;
	cin >> w >> m >> k;
	p = m_lg(m);
	t = m_pow(10, p);
	m--;
	while (w)
	{
		t--;
		if ((t - m) <= w / (p *k))
		{
			w -= (t - m) * p * k;
			ans += (t - m);
			m = t;
		}
		else
		{
			ans += w / (p * k);
			w = 0;
		}
		p++;
		t = (t + 1) * 10;
	}

	cout << ans << endl;
	return 0;
}