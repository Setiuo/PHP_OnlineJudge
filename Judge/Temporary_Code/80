#include <stdio.h>
#include <string.h>

int main()
{
	char Str[100];

	gets(Str);

	int len = strlen(Str);
	char Ans[100];

	int i = 0;
	for (; i < len; i++)
	{
		if (Str[i] >= 'a' && Str[i] <= 'z')
		{
			int iASCII = (Str[i] - 'a' + 3) % 26;

			Ans[i] = iASCII + 'A';
		}
		else if (Str[i] >= 'A' && Str[i] <= 'Z')
		{
			int iASCII = (Str[i] - 'A' + 3) % 26;

			Ans[i] = iASCII + 'a';
		}
	}

	while (i--)
	{
		printf("%c", Ans[i]);
	}
	return 0;
}