#include <stdio.h>
#include <math.h>

int GetNum(int num)
{
	int ans = 0;

	while (num != 0) {
		ans++;
		num /= 10;
	}

	return ans;
}

int main()
{
	int n;
	scanf("%d", &n);

	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= (n - i); j++)
		{
			printf("    ");
		}

		for (int j = 1; j <= i; j++)
		{
			double iNum = pow(2, j - 1);

			printf("%.0f", iNum);

			int Num = GetNum((int)iNum);
			for (int m = 1; m <= 4 - Num; m++)
			{
				printf(" ");
			}
		}
		for (int j = i - 1; j >= 1; j--)
		{
			double iNum = pow(2, j - 1);

			printf("%.0f", iNum);

			int Num = GetNum((int)iNum);
			for (int m = 1; m <= 4 - Num; m++)
			{
				printf(" ");
			}
		}
		printf("\n");
	}
	return 0;
}
