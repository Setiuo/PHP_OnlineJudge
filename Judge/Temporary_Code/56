import java.util.Scanner;
public class  Main
{
	public static void main(String[] args) 
	{
		int a=0;
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		for(int i=1;i<n/2;i++)
		{
			for(int j=n;j>i;j--)
			{
				if (i*j==n)
				{
                    a+=2;
				}
			}
		}
		System.out.println(a);
		
	}
}