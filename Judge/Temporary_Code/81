#include <stdio.h>
#include <string.h>

int main()
{
	int n;
	scanf("%d", &n);

	while (n--)
	{
		char Str[50];
		scanf("%s", Str);

		int len = strlen(Str);

		for (int i = 0; i < len; i++)
		{
			if (Str[i] >= 'a' && Str[i] <= 'z')
			{
				if (i == 0)
				{
					printf("%c", 'A' + Str[i] - 'a');
				}
				else
				{
					printf("%c", Str[i]);
				}
			}
			else if (Str[i] >= 'A' && Str[i] <= 'Z')
			{
				if (i == 0)
				{
					printf("%c", Str[i]);
				}
				else
				{
					printf("%c", 'a' + Str[i] - 'A');
				}
			}
			else
			{
				printf("%c", Str[i]);
			}
		}
		printf("\n");
	}
	
	return 0;
}
